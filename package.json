{
  "name": "@culturehq/components",
  "version": "15.0.1",
  "description": "CultureHQ's component library",
  "main": "dist/components.js",
  "scripts": {
    "build:js": "babel src --out-dir ./dist --ignore '**/__tests__' --ignore '**/__docs__' && cp src/*.json dist",
    "build:css": "node-sass src/styles/app.scss > dist/main.css",
    "clean": "git ls-files -o | grep -v node_modules | xargs rm",
    "docs": "build-storybook -s ./stories/public -o ./docs",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "chq-scripts lint",
    "lint:css": "sass-lint -c .sass-lint.yml -v --max-warnings 0",
    "prepublishOnly": "rm -rf dist && mkdir dist && yarn build:css && yarn build:js",
    "start": "start-storybook -p 6006 -s ./stories/public",
    "test": "TZ=UTC chq-scripts test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CultureHQ/components.git"
  },
  "author": "Kevin Deisz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CultureHQ/components/issues"
  },
  "homepage": "https://github.com/CultureHQ/components#readme",
  "peerDependencies": {
    "react": ">= 16.9",
    "react-dom": ">= 16.9"
  },
  "dependencies": {
    "cropperjs": "^1.5.4",
    "react-modal": "^3.8.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@culturehq/scripts": "^4.0.2",
    "@storybook/addon-a11y": "^5.1.11",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-knobs": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-storysource": "^5.1.11",
    "@storybook/addons": "^5.1.11",
    "@storybook/cli": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@storybook/theming": "^5.1.11",
    "@testing-library/react": "^9.1.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-axe": "^3.2.0",
    "node-sass": "^4.12.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.2.0",
    "webpack": "^4.39.3"
  },
  "eslintConfig": {
    "extends": [
      "@culturehq",
      "@culturehq/eslint-config/react"
    ],
    "rules": {
      "import/no-extraneous-dependencies": "off"
    }
  },
  "jest": {
    "moduleNameMapper": {
      "\\.s?css$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "testRegex": ".test.js",
    "testURL": "http://localhost"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
