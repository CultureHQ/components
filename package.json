{
  "name": "@culturehq/components",
  "version": "12.3.0",
  "description": "CultureHQ's component library",
  "main": "dist/components.js",
  "scripts": {
    "build": "yarn build:css && yarn build:js && yarn build:docs",
    "build:js": "babel src --out-dir ./dist --ignore '**/__tests__' --ignore '**/__docs__' && cp src/*.json dist",
    "build:css": "node-sass src/styles/app.scss > dist/main.css",
    "build:docs": "webpack --mode production",
    "clean": "git ls-files -o | grep -v node_modules | xargs rm",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint .",
    "lint:css": "sass-lint -c .sass-lint.yml -v --max-warnings 0",
    "start": "webpack-dev-server --mode development --hot",
    "test": "TZ=UTC jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CultureHQ/components.git"
  },
  "author": "Kevin Deisz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CultureHQ/components/issues"
  },
  "homepage": "https://github.com/CultureHQ/components#readme",
  "peerDependencies": {
    "react": "^16",
    "react-dom": "^16"
  },
  "dependencies": {
    "cropperjs": "^1.5.1",
    "react-modal": "^3.6.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@culturehq/eslint-config": "^1.0.2",
    "@mapbox/jsxtreme-markdown-loader": "^0.8.3",
    "@storybook/cli": "^5.0.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-codegen": "^3.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "css-loader": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint": "^5.14.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-axe": "^3.1.1",
    "node-sass": "^4.11.0",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.1.0",
    "storybook": "^1.0.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.13",
    "@storybook/react": "^5.0.5",
    "@storybook/addon-actions": "^5.0.5",
    "@storybook/addon-links": "^5.0.5",
    "@storybook/addons": "^5.0.5"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.scss$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ],
    "testRegex": ".test.js",
    "testURL": "http://localhost"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ]
}
