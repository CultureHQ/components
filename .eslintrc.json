{
  "extends": "@culturehq",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [
          ".js",
          ".mdx"
        ]
      }
    }
  }
}
